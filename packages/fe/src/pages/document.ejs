<!doctype html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
  <link rel="manifest" href="manifest.webmanifest">
</head>
<body>
  <div id="root"></div>
  <script>
    if (navigator.serviceWorker) {
      navigator.serviceWorker.register('service-worker.js', {
        scope: '/'
      });
    }
  </script>
</body>
</html>
